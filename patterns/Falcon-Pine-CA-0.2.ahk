loop reps {

        ;-------------]
        ;Corner Aligne
        ;-------------]

        ;foreward
        Send "{" TCFBKey " down}"
	Walk(20)
	Send "{" TCFBKey " up}"

        ;Right Corner Aligne
	Send "{" AFCLRKey " down}"
	Walk(20)
	Send "{" AFCLRKey " up}"

        ;Left Corner Aligne
        Send "{" TCLRKey " down}"
	Walk(6)
	Send "{" TCLRKey " up}"

        ;Backwards Corner Aligne
        Send "{" AFCFBKey " down}"
	Walk(6)
	Send "{" AFCFBKey " up}"

loop 11  {
        ;--------]
        ;Start Falcon
        ;--------]
    
        ;Forward
        Send "{" TCFBKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
    
        ;Right
        Send "{" AFCLRKey " down}"
        Walk(4)
        Send "{" AFCLRKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Left
        Send "{" TCLRKey " down}"
        Walk(4)
        Send "{" TCLRKey " up}"
    
        ;Forward
        Send "{" TCFBKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
        
        ;up-Right corner
    
        Send "{" TCFBKey " down}"
        Send "{" AFCLRKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
        Send "{" AFCLRKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Right-center
    
        Send "{" TCLRKey " down}"
        Walk(4)
        Send "{" TCLRKey " up}"
    
        ;Down-Right corner
    
        Send "{" AFCFBKey " down}"
        Send "{" AFCLRKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
        Send "{" AFCLRKey " up}"
    
        ;left
        Send "{" TCLRKey " down}"
        Walk(2)
        Send "{" TCLRKey " up}"
    
        ;Forward [center]
        Send "{" TCFBKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
    
        ;------------------]
        ; Right Part End
        ;------------------]
    
        ;Forward
        Send "{" TCFBKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
    
        ;Left
        Send "{" TCLRKey " down}"
        Walk(4)
        Send "{" TCLRKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Right
        Send "{" AFCLRKey " down}"
        Walk(4)
        Send "{" AFCLRKey " up}"
    
        ;Forward
        Send "{" TCFBKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
        
        ;up-Left corner
    
        Send "{" TCFBKey " down}"
        Send "{" TCLRKey " down}"
        Walk(4)
        Send "{" TCFBKey " up}"
        Send "{" TCLRKey " up}"
    
        ;Backwards
        Send "{" AFCFBKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
    
        ;Left-center
    
        Send "{" AFCLRKey " down}"
        Walk(4)
        Send "{" AFCLRKey " up}"
    
        ;Down-Left corner
    
        Send "{" AFCFBKey " down}"
        Send "{" TCLRKey " down}"
        Walk(4)
        Send "{" AFCFBKey " up}"
        Send "{" TCLRKey " up}"
    
        ;Right
        Send "{" AFCLRKey " down}"
        Walk(2)
